{
  "name": "moseq-reports",
  "version": "0.0.1",
  "author": "Tischfield Lab",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "package": "npm run electron:build -- --publish never",
    "package-ci": "npm run electron:build -- --publish always",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "version": "cat package.json | grep version | head -1 | awk -F: '{ print $2 }' | sed 's/[\", ]//g'",
    "preinstall": "npx npm-force-resolutions"
  },
  "resolutions": {
    "minimist": "1.2.3"
  },
  "build": {
    "productName": "moseq-reports",
    "appId": "org.tischfield.reports",
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 420,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis",
        "msi"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "AppImage"
      ],
      "category": "Development"
    },
    "publish": {
      "provider": "github",
      "owner": "tischfieldlab",
      "repo": "moseq-reports",
      "private": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tischfieldlab/moseq-reports.git"
  },
  "main": "background.js",
  "dependencies": {
    "chai-as-promised": "^7.1.1",
    "core-js": "^3.6.4",
    "d3": "^5.15.0",
    "data-forge": "^1.8.3",
    "electron-store": "^5.1.1",
    "file-saver": "^2.0.2",
    "hclusterjs": "^1.3.1",
    "html-to-image": "^0.1.1",
    "jquery": "^3.4.1",
    "jqwidgets-scripts": "^9.0.0",
    "node-stream-zip": "^1.9.1",
    "plotly.js-dist": "^1.52.3",
    "popper.js": "^1.16.1",
    "register-service-worker": "^1.7.1",
    "save-svg-as-png": "^1.4.17",
    "ts-debounce": "^2.0.1",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-color": "^2.7.1",
    "vue-drag-resize": "^1.3.2",
    "vue-property-decorator": "^8.4.1",
    "vue-resize": "^0.4.5",
    "vue-router": "^3.1.6",
    "vue-typed-mixins": "^0.2.0",
    "vue-unique-id": "^3.0.0",
    "vuedraggable": "^2.23.2",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@types/bootstrap": "^4.3.1",
    "@types/chai": "^4.2.11",
    "@types/d3": "^5.7.2",
    "@types/file-saver": "^2.0.1",
    "@types/mocha": "^7.0.2",
    "@types/plotly.js": "^1.44.33",
    "@vue/cli-plugin-babel": "^4.2.3",
    "@vue/cli-plugin-e2e-nightwatch": "^4.2.3",
    "@vue/cli-plugin-pwa": "^4.2.3",
    "@vue/cli-plugin-typescript": "^4.2.3",
    "@vue/cli-plugin-unit-mocha": "^4.2.3",
    "@vue/cli-service": "^4.2.3",
    "@vue/test-utils": "^1.0.0-beta.32",
    "bootstrap-vue": "^2.7.0",
    "canvas": "^2.5.0",
    "chai": "^4.2.0",
    "chromedriver": "^80.0.1",
    "dataframe-js": "^1.4.3",
    "electron": "^8.1.1",
    "geckodriver": "^1.19.1",
    "lodash": "^4.17.15",
    "lodash.defaultsdeep": "^4.6.1",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "selenium-server": "^3.141.59",
    "typescript": "^3.8.3",
    "vue-cli-plugin-electron-builder": "^1.4.6",
    "vue-devtools": "^5.1.3",
    "vue-template-compiler": "^2.6.11"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
